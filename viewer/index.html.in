<html id="openSeadragonApp" ng-app="openSeadragonapp">
    <head>
        <title>Data Viewer | (Re)Building A Kidney</title>
        %ASSETS%
        <meta http-equiv="X-UA-Compatible" content="IE=10; IE=9; IE=8; IE=7; IE=EDGE" />
    </head>
    <body>

    <!-- HEADER -->
    <div ng-controller="HeaderCtrl" id="header" class="navbar navbar-fixed-top">
        <div class="navbar-header"><a id="brand" href="/" class="navbar-brand">Chaise</a></div>
	    <div class="collapse navbar-collapse navbar-inverse" id="fb-navbar-main-collapse">

               <p class="navbar-text pull-right">
               	<a class="login" id="login_link" href="javascript:initLogin()">Log in</a>
               	<a id="login_user" href="#"></a>
               	<a class="login" id="logout_link" style="display:none;" href="javascript:deleteSession(window.location)">Logout</a>
               	<i class="md-account-circle md-lg"></i> Profile
               </p>

	    </div><!-- /.navbar-collapse -->
    </div>
    <!-- SPINNER -->
    <div ng-init="spinnerVisible = false" ng-show="spinnerVisible" id="spinner">
        <img src="assets/images/spinner.gif" style="display: block; margin: 0 auto; width: 32px; height: 32px; opacity: 0.8;"/>
        <div style="margin-top: 15px;">{{ spinnerText }}</div>
    </div>
    <!-- NOT FOUND -->
    <div ng-init="notFoundVisible = false" ng-show="notFoundVisible" id="not-found">
        <div class="glyphicon glyphicon-search"></div> <br />
        <div id="not-found-title">Not Found</div> <br />
        <div id="not-found-subtitle">{{ notFoundText }}</div>
    </div>
    <div ng-controller="DetailCtrl" class="col-xs-10 col-xs-offset-1">
        <div ng-if="entity">
        <div id="record-bookmark-container" class="col-xs-12 meta-icons">
            <a title="This link stores your search criteria as a URL. Right click and save." ng-href="{{permanentLink()}}">
            <span class="glyphicon glyphicon-bookmark"></span> <strong>Permalink</strong>
            </a>
        </div>

            <!-- MAIN TABLE -->
            <iframe title="3dView" width="100%" height="400" ng-src="http://vm-dev-030.misd.isi.edu/~jessie/openseadragon-viewer/mview.html?url=http://vm-dev-030.misd.isi.edu/~mei/data/real3/DZC/DAPI/ImageProperties.xml&url=http://vm-dev-030.misd.isi.edu/~mei/data/real3/DZC/Alexa%20Fluor%20488/ImageProperties.xml&url=http://vm-dev-030.misd.isi.edu/~mei/data/real3/DZC/Alexa%20Fluor%20555/ImageProperties.xml&url=http://vm-dev-030.misd.isi.edu/~mei/data/real3/DZI/ImageProperties.xml&x=0.5&y=0.6452489905787349&z=0.45868459229311215" src="http://vm-dev-030.misd.isi.edu/~jessie/openseadragon-viewer/mview.html?url=http://vm-dev-030.misd.isi.edu/~mei/data/real3/DZC/DAPI/ImageProperties.xml&url=http://vm-dev-030.misd.isi.edu/~mei/data/real3/DZC/Alexa%20Fluor%20488/ImageProperties.xml&url=http://vm-dev-030.misd.isi.edu/~mei/data/real3/DZC/Alexa%20Fluor%20555/ImageProperties.xml&url=http://vm-dev-030.misd.isi.edu/~mei/data/real3/DZI/ImageProperties.xml&x=0.5&y=0.6452489905787349&z=0.45868459229311215">&lt;p&gt;Your browser does not support iframes.&lt;/p&gt;</iframe>
            <div id="entity-title">{{ entity.internal.displayTitle }}</div>
            <div id="entity-subtitle">{{ entity.internal.displayTableName }}</div>
            <table class="table">
                <tr ng-repeat="(key, value) in entity | filteredEntity">
                    <td class="entity-key col-xs-2">{{ key | removeUnderScores }}</td>
                    <td class="entity-value col-xs-10" ng-bind-html="value | sanitizeValue"></td>
                </tr>
                <tr ng-repeat="association in entity.associations">
                    <td class="entity-key col-xs-2">{{ association.referencedTableName | removeUnderScores }}</td>
                    <td class="entity-value col-xs-10">
                        <ul class="entity-terms">
                            <li class="entity-term" ng-repeat="term in association.terms">
                                <a ng-href="{{term.link}}" ng-click="reloadPage()">{{term.vocab}}</a>
                            </li>
                        </ul>
                    </td>
                </tr>
            </table>

            <accordion close-others="oneAtATime">

                <!-- IMAGES -->
                <accordion-group ng-if="entity.images" ng-init="images.open = true" is-open="images.open">
                <accordion-heading><div ng-class="{'active': images.open }">Images</div></accordion-heading>
                <div ng-controller="ImagesCtrl">
                    <ul id="entity-images">
                        <li ng-repeat="image in entity.images" class="image" on-finish-render>
                            <!-- <a href="slide{{ $index + 1 }}"> -->
                            <a class="fancybox" href="{{ image.uri }}">
                                <img ng-src="{{ image.uri }}" class="image-thumbnail" slippryimageonload />
                            </a>
                        </li>
                    </ul>
                    <div id="thumbnail-box" class="clearfix">
                      <ul class="thumbs" class="clearfix">
                            <li ng-repeat="imagea in entity.images" \>
                                <a href="" data-att-slide="{{ $index + 1 }}">
                                    <img ng-src="{{ imagea.uri }}" />
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
                </accordion-group>

                <!-- FILES -->
                <accordion-group ng-if="entity.files" is-open="files.open">
                    <accordion-heading><div ng-class="{'active': files.open }">Files</div></accordion-heading>
                    <ul>
                        <li ng-repeat="file in entity.files" class="file" >
                            <a href="{{ file.uri }}" target="_blank">
                                <img ng-src="{{ file.content_type | iconPreviewUri }}" class="file-preview-icon">
                                <div>{{ file.filename }}</div>
                                <div class="file-size">{{ file.bytes | filesize }}</div>
                            </a>
                        </li>
                    </ul>
                </accordion-group>

                <!-- PREVIEWS -->
                <accordion-group ng-if="entity.previews" is-open="previews.open">
                    <accordion-heading><div ng-class="{'active': previews.open }">Previews</div></accordion-heading>
                    <ul id="previews">
                        <li ng-repeat="preview in entity.previews" class="preview col-sm-6 col-xs-12">
                            <div class="preview-content">
                                <a href="https://www.facebase.org/_viewer/xtk/view.html?url={{ preview.preview }}" target="_blank" class="enlarge">Enlarge</a>
                                <iframe title="3dView" width="100%" height="400" ng-src="https://www.facebase.org/_viewer/xtk/view_on_load.html?url=https://www.facebase.org/hatrac/facebase/previews/fb1/FB00000177/CP084CCMB_transformed.nii.gz" src="https://www.facebase.org/_viewer/xtk/view_on_load.html?url=https://www.facebase.org/hatrac/facebase/previews/fb1/FB00000177/CP084CCMB_transformed.nii.gz">&lt;p&gt;Your browser does not support iframes.&lt;/p&gt;</iframe>
                            </div>
                            <a class="preview-filename" href="{{ preview.uri }}">{{ preview.filename }}</a>
                            <div class="file-size">{{ preview.bytes | filesize }}</div>
                        </li>
                    </ul>
                </accordion-group>

                <!-- REFERENCES -->
                <accordion-group ng-repeat="ft in entity.foreignTables | orderBy:'title'" is-open="ft.open">

                    <accordion-heading>
                        <div ng-class="{'active': ft.open }" ng-click="foreignTableToggle($index)">
                            {{ ft.title | removeUnderScores }}
                            <span ng-if="ft.count > 0">({{ ft.count }})</span>
                        </div>

                    </accordion-heading>

                    <div id="ft-transpose" ng-controller="NestedTablesCtrl" ng-if="ft.transpose">
                        <ul class="reference-views clearfix" ng-if="ft.count > 0">
                            <li><div class="reference-view-title">View</div></li>
                            <li><a ng-click="ft.transpose = false" ng-class="{ 'active': !ft.transpose }" class="transpose-toggle">Default</a></li>
                            <li><a ng-click="ft.transpose = true" ng-class="{ 'active': ft.transpose }" class="transpose-toggle">Transpose</a></li>
                        </ul>

                        <p ng-if="ft.count == 0">No rows found</p>

                        <div class="table-wrapper wrapper">
                            <table class="table table-striped table-reference">
                                <tbody>
                                    <tr ng-repeat="key in ft.keys" on-finish-render>
                                        <th class="entity-key col-xs-2">{{ key | removeUnderScores }}</th>
                                        <td ng-repeat="reference in ft.list" class="entity-value col-xs-10">
                                            <a ng-if="reference[key + '_link']" ng-href="{{reference[key + '_link']}}" ng-click="reloadPage()">
                                                {{reference[key] | sanitizeValue }}
                                            </a>
                                            <span ng-if="!reference[key + '_link']" ng-bind-html="reference[key] | sanitizeValue"></span>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>

                    <div ng-controller="NestedTablesCtrl" ng-if="!ft.transpose">

                        <ul class="reference-views clearfix" ng-if="ft.count > 0">
                            <li><div class="reference-view-title">View</div></li>
                            <li><a ng-click="ft.transpose = false" ng-class="{ 'active': !ft.transpose }" class="transpose-toggle">Default</a></li>
                            <li><a ng-click="ft.transpose = true" ng-class="{ 'active': ft.transpose }" class="transpose-toggle">Transpose</a></li>
                        </ul>

                        <p ng-if="ft.count == 0">No rows found</p>

                        <div class="table-wrapper wrapper">
                            <table class="table table-striped nested table-reference">
                                <thead>
                                    <tr>
                                        <th ng-repeat="key in ft.keys" class="entity-key col-xs-2">{{ key | removeUnderScores }}</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="reference in ft.list" on-finish-render>
                                        <td ng-repeat="key in ft.keys"  class="entity-value col-xs-10" >
                                            <a ng-if="reference[key + '_link']" ng-href="{{reference[key + '_link']}}" ng-click="reloadPage()">
                                                {{reference[key] | sanitizeValue }}
                                            </a>
                                            <span ng-if="!reference[key + '_link']" ng-bind-html="reference[key] | sanitizeValue"></span>

                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>

                    </div>

                </accordion-group>

            </accordion>
        <!-- NG-IF ENTITY -->
        </div>
    <!-- DETAIL CONTROLLER -->
    </div>
    </body>
</html>
